manifest {
    mainScript = "main.nf"
    defaultBranch = "master"
    nextflowVersion = ">=20.07.0"
}



params {
    conditions_file = ""
    launch_dir = ""
    trace_dir = "trace"
    trials = 1

    pipeline_name = ""
    pipeline_profiles = ""
    pipeline_output_dir = ""
    pipeline_trace_file = ""
}



process {
    errorStrategy = "ignore"
}



profiles {
    standard {
        process {
            executor = "local"
            cpus = 1
            memory = 4.GB
        }
        executor {
            queueSize = 1
        }
    }

    testing {
        process.errorStrategy = "terminate"
    }
}
